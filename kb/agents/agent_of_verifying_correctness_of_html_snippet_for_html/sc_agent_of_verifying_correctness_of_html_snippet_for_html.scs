agent_of_verifying_correctness_of_html_snippet_for_html
=> nrel_main_idtf:
	[ sc-агент проверки корректности html-фрагмента(с позиции HTML)] 
		(* <- lang_ru;; *);

	[sc-agent of verifying correctness of html snippet(HTML)] 
		(* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_verifying_correctness_of_html_snippet_for_html_condition => ..sc_agent_of_verifying_correctness_of_html_snippet_for_html_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_of_verifying_correctness_of_html_snippet_for_html
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_verifying_correctness_of_html_snippet_for_html
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_verifying_correctness_of_html_snippet_for_html;
		proc_checking_container_html;
		proc_checking_lists_html;
		proc_checking_autonomic_element
		};;
	-> sc_agent_of_verifying_correctness_of_html_snippet_for_html_scp   
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_verifying_correctness_of_html_snippet_for_html_condition
= [*
	action_of_verifying_correctness_of_html_snippet_for_html _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._param;;
*];;

..sc_agent_of_verifying_correctness_of_html_snippet_for_html_result
= [*
	action_of_verifying_correctness_of_html_snippet_for_html _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._param;;
*];;
